{
  "name": "lunasea-notification-service",
  "version": "1.3.8",
  "description": "Notification service for LunaSea",
  "repository": "https://github.com/JagandeepBrar/LunaSea-Notification-Service",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --config nodemon.json src/index.ts",
    "build": "tsc",
    "serve": "cross-env NODE_ENV=production ts-node -r dotenv/config dist/index.js",
    "publish": "npm run docker:build && npm run docker:push",
    "docker:start": "ts-node -r dotenv/config dist/index.js",
    "docker:build": "docker build -t ghcr.io/jagandeepbrar/lunasea-notification-service:latest -t ghcr.io/jagandeepbrar/lunasea-notification-service:v$(echo $npm_package_version) .",
    "docker:push": "docker push ghcr.io/jagandeepbrar/lunasea-notification-service:latest && docker push  ghcr.io/jagandeepbrar/lunasea-notification-service:v$(echo $npm_package_version)",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install"
  },
  "author": "Jagandeep Brar",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@types/basic-auth": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.8",
    "@types/node": "^17.0.21",
    "axios": "^0.26.0",
    "basic-auth": "^2.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "firebase-admin": "^10.0.2",
    "ioredis": "^4.28.5",
    "node-typescript-compiler": "2.3.0",
    "ts-node": "^10.5.0",
    "tslog": "^3.3.2",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "pretty-quick": "^3.1.3"
  }
}
